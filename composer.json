{
    "name": "combindma/gallery",
    "description": "Laravel Gallery Package",
    "keywords": [
        "combindma",
        "laravel",
        "gallery"
    ],
    "homepage": "https://github.com/combindma/gallery",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "webmaster@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "combindma/dashui": "^1.0",
        "combindma/flash": "^1.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-medialibrary": "^9.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\Gallery\\": "src",
            "Combindma\\Gallery\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\Gallery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "pest": "vendor/bin/pest",
        "pest-coverage": "vendor/bin/pest coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\Gallery\\GalleryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
